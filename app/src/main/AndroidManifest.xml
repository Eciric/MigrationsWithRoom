<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.migrationswithroom">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MigrationsWithRoom">
        <activity
            android:name=".Room.AddOwnerActivity"
            android:parentActivityName=".Room.RoomActivity" />
        <activity
            android:name=".Room.AddCategoryActivity"
            android:parentActivityName=".Room.RoomActivity" />
        <activity
            android:name=".Room.AddItemActivity"
            android:parentActivityName=".Room.RoomActivity" />
        <activity android:name=".SQLite.SqliteActivity"/>
        <activity android:name=".Room.RoomActivity" />
        <activity android:name=".Room.ExampleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>